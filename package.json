    "@angular/router": "^9.0.0-next.4",
    "@angular/service-worker": "^9.0.0-next.4",
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "@nestjs/platform-express": "^6.2.4",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/router-store": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
