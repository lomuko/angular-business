    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.4",
    "angular-http-server": "^1.8.1",
    "codelyzer": "^5.1.0",
    "cypress": "^3.4.1",
    "dotenv": "6.2.0",
    "jest": "24.1.0",
    "jest-preset-angular": "^7.0.1",
    "ngx-pwa-icons": "^0.1.2",
    "prettier": "1.15.2",
